<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="hc_base_style"
      parent="@android:style/Theme.Light.NoTitleBar">
    <item name="android:windowAnimationStyle">@style/HCWindowAnimTheme</item>
