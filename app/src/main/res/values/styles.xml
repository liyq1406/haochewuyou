  <style name="hc_splash_fullscreen_and_translucent"
      parent="android:Theme">
    <item name="android:windowBackground">@color/promote_white</item>
    <item name="android:windowNoTitle">true</item>
  </style>


  <style name="HCWindowAnimTheme"
      parent="@android:style/Animation.Activity">
    <item name="android:activityOpenEnterAnimation">@anim/trans_in_right_left</item>
    <item name="android:activityOpenExitAnimation">@anim/anim_activity_main</item>
    <item name="android:activityCloseEnterAnimation">@anim/anim_activity_main</item>
    <item name="android:activityCloseExitAnimation">@anim/trans_out_left_right</item>
  </style>

  <style name="DialogAnimation">
    <item name="android:windowEnterAnimation">@anim/anim_sub_tran_in</item>
    <item name="android:windowExitAnimation">@anim/anim_sub_tran_out</item>
  </style>


  <style name="HC_Search_style"
      parent="android:Theme.Dialog">
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:windowFrame">@null</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowIsFloating">true</item>
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:windowAnimationStyle">@style/hc_search_anim</item>
  </style>

